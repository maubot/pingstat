    "from pyvis.network import Network\n",
    "\n",
    "g = Network(height=\"1500px\", width=\"100%\", bgcolor=\"#222222\", font_color=\"white\")\n",
    "g.add_node(\"A\")\n",
    "g.add_node(\"B\")\n",
    "g.add_edge(\"A\", \"B\")\n",
    "\n",
    "print(g.generate_html())\n",
    "# g.show(\"test.html\")\n",
